{
  "name": "crossbuilder",
  "version": "0.8.0",
  "description": "Building web, Electron and Chrome apps, and cross-browser extensions with React, Redux and Webpack. 'Write once, build everywhere' concept in practice.",
  "scripts": {
    "start": "npm run build:dev",
    "build:production": "gulp build:production",
    "build:staging": "gulp build:staging",
    "build:dev": "gulp build:dev",
    "clean": "rm -rf build",
    "test": "LMEM_BACKEND_ORIGIN='' NODE_ENV=test mocha --compilers js:babel-core/register test/app --recursive",
    "lint": "git ls-files -om --exclude-standard | grep \\.js$ | xargs eslint --fix",
    "deploy:production": "gulp deploy:production",
    "deploy:staging": "gulp deploy:staging"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insitu-project/proto-ext"
  },
  "homepage": "http://www.lmem.net",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "live",
    "edit",
    "webpack",
    "flux",
    "redux",
    "firefox",
    "safari",
    "chrome",
    "app",
    "extension"
  ],
  "authors": [
    "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
    "Jhen <developer@jhen.me> (https://github.com/jhen0409)"
  ],
  "license": "MIT",
