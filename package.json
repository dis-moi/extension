{
  "name": "lmem-recommendations",
  "version": "1.1.0",
  "description": "LMEM Recommendations web extensions using React+Redux.",
  "scripts": {
    "start": "npm run build:dev",
    "build:production": "gulp build:production",
    "build:staging": "gulp build:staging",
    "build:dev": "gulp build:dev",
    "clean": "rm -rf build",
    "test": "LMEM_BACKEND_ORIGIN='' LMEM_SCRIPTS_ORIGIN='.' NODE_ENV=test mocha --compilers js:@babel/register test/app --recursive",
    "lint": "git ls-files -m | grep \\.js$ | xargs eslint --fix",
    "analyse": "babel-node ./node_modules/webpack/bin/webpack --optimize-minimize --mode production --config webpack/chromium.config.js --profile --json > stats.json"
  },
  "pre-commit": [
    "lint"
  ],
  "engines": {
    "node": ">=8"
  },
  "repository": {
