{
  "name": "lmem-recommendations",
  "version": "2.0.0",
  "description": "LMEM Recommendations web extensions using React+Redux.",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "webpack --watch --progress --hot --env.hmr --mode=development --env.build=dev",
    "build:staging": "webpack --mode=production --env.build=staging",
    "build:firefox": "webpack --mode=production --env.build=firefox",
    "build:chromium": "webpack --mode=production --env.build=chromium",
    "build:production": "yarn build:chromium && yarn build:firefox",
    "build": "yarn build:staging && yarn build:production",
    "clean": "rm -rf build",
    "test": "LMEM_BACKEND_ORIGIN='' NODE_ENV=test ts-mocha --paths test/app/*.ts test/app/**/*.ts",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint -p .",
    "lint:css": "stylelint './src/**/*.js'",
    "analyse": "babel-node ./node_modules/webpack/bin/webpack --optimize-minimize --mode production --config webpack/chromium.config.js --profile --json > stats.json",
    "storybook": "start-storybook -p 6007 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "tslint -p .",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insitu-project/recommendations-webextension"
  },
  "homepage": "https://www.lmem.net",
