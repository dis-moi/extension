{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "jsx-boolean-value": false,
    "no-unused-expression": false,
    "no-empty": [true, "allow-empty-functions"]
  },
  "jsRules": true,
  "rulesDirectory": []
}
